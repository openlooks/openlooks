    "dev": "node watch.cjs"
  },
  "devDependencies": {
    "@builder.io/mitosis": "0.0.92",
    "@builder.io/mitosis-cli": "0.0.51",
    "@openlooks/styles": "*",
    "@tabler/icons": "2.11.0",
    "chokidar": "3.5.3"
  }
}
