{
  "name": "@openlooks/mitosis",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run build:icons && npm run build:index && npm run build:mitosis && npm run build:copyfiles",
    "build:icons": "node buildicons.cjs",
    "build:index": "node buildindex.cjs",
    "build:mitosis": "mitosis build --config=mitosis.config.cjs",
